        <TextView android:id="@id/storage_debug_ui_button" android:text="@string/photos_settings_internal_storage_debug_ui" app:drawableStartCompat="@drawable/gs_storage_vd_theme_24" style="@style/photos_settings_main_page_item" />
        <include layout="@layout/photos_settings_header_and_footer_spacing" />
    </LinearLayout>
</ScrollView>