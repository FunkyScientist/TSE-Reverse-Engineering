<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="6dp"
        android:top="6dp"
        android:right="6dp"
        android:bottom="6dp">
        <shape android:shape="oval">
            <solid android:color="?android:attr/colorBackground"/>
            <size
                android:height="20dp"
                android:width="20dp"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:id="@+id/error_icon"
        android:tint="?attr/photosStorageFull"
        android:drawable="@drawable/quantum_gm_ic_error_vd_theme_24"/>
</layer-list>
