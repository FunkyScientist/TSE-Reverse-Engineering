<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/photos_assistant_cardui_photo_grid_padding">
        <com.google.android.apps.photos.assistant.cardui.CardPhotoView
            android:id="@+id/photo_0"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/photos_assistant_cardui_two_photos_min_height"
            android:layout_weight="1"
            android:contentDescription="@string/accessibility_photo_button"
            android:layout_marginEnd="@dimen/photos_assistant_cardui_photo_grid_padding"/>
        <com.google.android.apps.photos.assistant.cardui.CardPhotoView
            android:id="@+id/photo_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/photos_assistant_cardui_two_photos_min_height"
            android:layout_weight="1"
            android:contentDescription="@string/accessibility_photo_button"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.google.android.apps.photos.assistant.cardui.CardPhotoView
            android:id="@+id/photo_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/photos_assistant_cardui_three_photos_min_height"
            android:layout_weight="1"
            android:contentDescription="@string/accessibility_photo_button"
            android:layout_marginEnd="@dimen/photos_assistant_cardui_photo_grid_padding"/>
        <com.google.android.apps.photos.assistant.cardui.CardPhotoView
            android:id="@+id/photo_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/photos_assistant_cardui_three_photos_min_height"
            android:layout_weight="1"
            android:contentDescription="@string/accessibility_photo_button"
            android:layout_marginEnd="@dimen/photos_assistant_cardui_photo_grid_padding"/>
        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1">
            <com.google.android.apps.photos.assistant.cardui.CardPhotoView
                android:id="@+id/photo_4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:minHeight="@dimen/photos_assistant_cardui_three_photos_min_height"
                android:contentDescription="@string/accessibility_photo_button"/>
            <TextView
                android:gravity="center"
                android:id="@+id/overflow_count"
                android:background="@color/photos_assistant_cardui_overlay"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textIsSelectable="false"
                style="@style/quantum_text_title_white"/>
        </FrameLayout>
    </LinearLayout>
</merge>
