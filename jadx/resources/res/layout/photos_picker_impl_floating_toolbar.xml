<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.floatingsearchbar.SearchBarLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/toolbar_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/floating_toolbar_card_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        style="@style/FloatingSearchBarCardView">
        <android.support.v7.widget.Toolbar
            android:theme="@style/ThemeOverlay.Photos.ActionBar.FloatingToolbar"
            android:id="@+id/floating_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/photos_floatingsearchbar_height"
            android:touchscreenBlocksFocus="false"
            app:buttonGravity=""
            app:contentInsetStart="0dp"/>
    </com.google.android.material.card.MaterialCardView>
</com.google.android.apps.photos.floatingsearchbar.SearchBarLayout>
