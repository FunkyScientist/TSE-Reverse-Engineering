<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <FrameLayout
        android:clipChildren="false"
        android:layout_width="match_parent"
        android:layout_height="194dp">
        <ViewStub
            android:layout_gravity="bottom"
            android:id="@+id/photos_photoeditor_fragments_editor3_interstitial_trim_scrubber_viewstub"
            android:layout="@layout/photos_photoeditor_fragments_editor3_scrubber"
            android:inflatedId="@+id/photos_photoeditor_fragments_editor3_interstitial_trim_scrubber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"/>
    </FrameLayout>
    <android.support.v7.widget.Toolbar
        android:layout_gravity="bottom"
        android:id="@+id/toolbar"
        android:background="@color/google_black"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_margin="0dp"
        android:contentInsetStart="0dp"
        android:contentInsetEnd="0dp"
        app:contentInsetEnd="0dp"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        style="?attr/actionBarStyle">
        <com.google.android.material.button.MaterialButton
            android:layout_gravity="start"
            android:id="@+id/photos_photoeditor_fragments_effects_interstitial_trim_back_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/photos_strings_back_button"
            android:layout_marginStart="4dp"
            style="@style/photos_photoeditor_fragments_editor3_inline_button"/>
        <com.google.android.material.button.MaterialButton
            android:enabled="false"
            android:layout_gravity="end"
            android:id="@+id/photos_photoeditor_fragments_effects_interstitial_trim_next_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/photos_strings_next_button"
            android:layout_marginEnd="12dp"
            style="@style/Widget.Photos.Button.StrokedButton"/>
    </android.support.v7.widget.Toolbar>
</LinearLayout>
