<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/story_card_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="0dp"
    android:paddingEnd="0dp">
    <ImageView
        android:id="@+id/spotlight_image"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        android:contentDescription="@null"
        app:layout_constraintDimensionRatio="h,4:5"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/scrim_guideline"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:layout_constraintBottom_toTopOf="@+id/photos_story_card_subtitle"/>
    <ImageView
        android:id="@+id/scrim"
        android:background="@drawable/photos_flyingsky_spotlight_image_scrim"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="@+id/spotlight_image"
        app:layout_constraintEnd_toEndOf="@+id/spotlight_image"
        app:layout_constraintStart_toStartOf="@+id/spotlight_image"
        app:layout_constraintTop_toTopOf="@+id/scrim_guideline"/>
    <ImageView
        android:id="@+id/top_wavy_divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/photos_flyingsky_wavy_divider_height"
        android:contentDescription="@null"
        app:layout_constraintTop_toTopOf="@+id/spotlight_image"/>
    <ImageView
        android:id="@+id/bottom_wavy_divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/photos_flyingsky_wavy_divider_height"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="@+id/spotlight_image"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.BodyMedium"
        android:textColor="@color/photos_flyingsky_story_card_default_subtitle_color"
        android:ellipsize="end"
        android:gravity="start"
        android:id="@+id/photos_story_card_subtitle"
        android:paddingLeft="24dp"
        android:paddingRight="24dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLines="2"
        android:lineSpacingExtra="1sp"
        android:textAlignment="viewStart"
        android:letterSpacing="0.01"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:theme="@style/ThemeOverlay.Photos.DayNight.Primary"
        android:textAppearance="@style/TextAppearance.Photos.Subhead1"
        android:textSize="11sp"
        android:ellipsize="end"
        android:gravity="start"
        android:id="@+id/photos_story_card_sharing_attributions"
        android:paddingLeft="24dp"
        android:paddingRight="24dp"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLines="2"
        android:lineSpacingExtra="1sp"
        android:textAlignment="viewStart"
        android:letterSpacing="0.01"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_title"/>
    <TextView
        android:theme="@style/ThemeOverlay.Photos.DayNight.Primary"
        android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
        android:ellipsize="end"
        android:gravity="start"
        android:id="@+id/photos_story_card_sharing_attributions_clickable"
        android:paddingTop="12dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="2"
        android:lineSpacingExtra="1sp"
        android:textAlignment="viewStart"
        android:paddingEnd="24dp"
        android:layout_marginStart="24dp"
        android:letterSpacing="0.01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_title"/>
    <FrameLayout
        android:id="@+id/title_suggestion_button_view"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:layout_height="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:foreground="?attr/selectableItemBackground"
        android:contentDescription="@string/photos_strings_untitled_album_hint_text"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/photos_story_card_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/photos_story_card_title">
        <com.airbnb.lottie.LottieAnimationView
            android:layout_gravity="center"
            android:id="@+id/title_suggestion_button_view_background"
            android:layout_width="@dimen/photos_flyingsky_suggestion_card_action_button_size"
            android:layout_height="@dimen/photos_flyingsky_suggestion_card_action_button_size"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/photos_ellmann_icon_background_animation"/>
        <ImageButton
            android:layout_gravity="center"
            android:background="@null"
            android:focusable="false"
            android:clickable="false"
            android:layout_width="@dimen/photos_flyingsky_suggestion_card_action_button_size"
            android:layout_height="@dimen/photos_flyingsky_suggestion_card_action_button_size"
            android:tint="?attr/colorOnSurface"
            android:contentDescription="@null"
            app:srcCompat="@drawable/photos_quantum_gm_gs_pen_spark_vd_theme_18"/>
    </FrameLayout>
    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="24dp"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_subtitle"
        layout="@layout/photos_flyingsky_ui_ongoing_chip"/>
    <TextView
        android:textAppearance="@style/FlyingSkyHeadlineMediumBold"
        android:textColor="@color/photos_flyingsky_story_card_default_title_color"
        android:ellipsize="end"
        android:gravity="start"
        android:id="@+id/photos_story_card_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/photos_flyingsky_title_top_margin"
        android:hint="@string/photos_strings_untitled_album_hint_text"
        android:maxLines="3"
        android:textAlignment="viewStart"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/title_suggestion_button_view_text"
        app:layout_constraintStart_toEndOf="@+id/title_suggestion_button_view"
        app:layout_constraintTop_toBottomOf="@+id/ongoing_chip"
        app:layout_goneMarginEnd="@dimen/photos_flyingsky_title_end_margin"
        app:layout_goneMarginStart="@dimen/photos_flyingsky_title_start_margin"/>
    <com.airbnb.lottie.LottieAnimationView
        android:layout_gravity="center"
        android:id="@+id/title_suggestion_button_view_background_text"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/title_suggestion_button_view_text"
        app:layout_constraintEnd_toEndOf="@+id/title_suggestion_button_view_text"
        app:layout_constraintStart_toStartOf="@+id/title_suggestion_button_view_text"
        app:layout_constraintTop_toTopOf="@+id/title_suggestion_button_view_text"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/photos_ellmann_icon_background_animation"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.LabelLarge"
        android:textColor="?attr/colorOnSurface"
        android:ellipsize="end"
        android:gravity="center"
        android:layout_gravity="center"
        android:id="@+id/title_suggestion_button_view_text"
        android:background="@null"
        android:focusable="true"
        android:visibility="gone"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:foreground="?attr/selectableItemBackground"
        android:text="@string/photos_flyingsky_fragment_generate_title"
        android:maxLines="1"
        android:drawablePadding="8dp"
        android:paddingStart="16dp"
        android:paddingEnd="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="24dp"
        android:drawableTint="?attr/colorOnSurface"
        app:drawableStartCompat="@drawable/photos_quantum_gm_gs_pen_spark_vd_theme_18"
        app:layout_constrainedHeight="true"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toBottomOf="@+id/photos_story_card_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/photos_story_card_title"
        app:layout_constraintWidth_max="170dp"/>
    <ViewStub
        android:id="@+id/photos_story_card_title_edit_stub"
        android:layout="@layout/photos_flyingsky_story_card_title_edit_text"
        android:inflatedId="@+id/photos_story_card_title_edit_stub_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/photos_flyingsky_title_top_margin"
        android:layout_marginStart="@dimen/photos_flyingsky_title_start_margin"
        android:layout_marginEnd="@dimen/photos_flyingsky_title_end_margin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_subtitle"/>
    <include
        android:id="@+id/pregenerated_titles_chips_container"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_suggestion_button_view"
        layout="@layout/photos_flyingsky_ui_ellmann_titlesuggestion_pregenerated_chips_view"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.BodySmall"
        android:textColor="@color/photos_flyingsky_story_card_default_narrative_color"
        android:ellipsize="end"
        android:gravity="start"
        android:id="@+id/photos_story_card_narrative"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:maxLines="3"
        android:textAlignment="viewStart"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/title_suggestion_button_view"
        app:layout_constraintTop_toBottomOf="@+id/pregenerated_titles_chips_container"
        app:layout_goneMarginStart="@dimen/photos_flyingsky_title_start_margin"/>
    <RelativeLayout
        android:id="@+id/sharing_chips_container"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/overflow_button_tap_target"
        app:layout_constraintEnd_toStartOf="@+id/share_button_tap_target"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="@+id/title_suggestion_button_view"
        app:layout_constraintTop_toTopOf="@+id/overflow_button_tap_target"
        app:layout_goneMarginStart="@dimen/photos_flyingsky_title_start_margin">
        <include
            android:id="@+id/suggest_invite_partner_chip"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            layout="@layout/photos_sharesuggestion_shortcut_chip_lsv"/>
        <include
            android:id="@+id/suggest_share_with_faces_chip_var_sharememory"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            layout="@layout/photos_sharesuggestion_facecluster_chip_lsv_sharememory"/>
        <include
            android:id="@+id/suggest_share_with_faces_chip_var_sharewith"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            layout="@layout/photos_sharesuggestion_facecluster_chip_lsv_sharewith"/>
    </RelativeLayout>
    <View
        android:id="@+id/cloud_grid_spacer"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/photos_flyingsky_ui_cloud_grid_top_margin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_narrative"/>
    <com.google.android.apps.photos.flyingsky.cloudgrid.p012ui.CloudGridView
        android:id="@+id/photos_story_card_cloud_grid"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cloud_grid_spacer"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.BodyLarge"
        android:textColor="@color/google_white"
        android:gravity="center"
        android:id="@+id/item_count"
        android:focusable="false"
        android:visibility="gone"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@null"
        android:textAlignment="center"
        android:layout_marginEnd="@dimen/photos_flyingsky_ui_cloud_grid_tap_target_button_end_margin"
        app:layout_constraintBottom_toBottomOf="@+id/photos_story_card_cloud_grid"
        app:layout_constraintEnd_toEndOf="@+id/photos_story_card_cloud_grid"/>
    <FrameLayout
        android:id="@+id/overflow_button_tap_target"
        android:layout_width="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:layout_height="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:contentDescription="@string/photos_flyingsky_fragment_highlight_overflow_button_description"
        android:layout_marginEnd="@dimen/photos_flyingsky_ui_cloud_grid_tap_target_button_end_margin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_cloud_grid">
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/overflow_button"
            android:background="@drawable/photos_flyingsky_overflow_button"
            android:focusable="false"
            android:visibility="invisible"
            android:clickable="false"
            android:layout_width="@dimen/photos_flyingsky_overflow_button_size"
            android:layout_height="@dimen/photos_flyingsky_overflow_button_size"
            android:scaleType="center"
            android:contentDescription="@null"
            app:srcCompat="@drawable/photos_quantum_gm_ic_more_vert_vd_theme_18"/>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/share_button_tap_target"
        android:layout_width="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:layout_height="@dimen/photos_flyingsky_miniminum_gar_dimension"
        android:contentDescription="@string/photos_flyingsky_share_button_text"
        app:layout_constraintEnd_toStartOf="@+id/overflow_button_tap_target"
        app:layout_constraintTop_toBottomOf="@+id/photos_story_card_cloud_grid">
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/share_button"
            android:background="@drawable/photos_flyingsky_overflow_button"
            android:paddingLeft="-1dp"
            android:focusable="false"
            android:clickable="false"
            android:layout_width="@dimen/photos_flyingsky_overflow_button_size"
            android:layout_height="@dimen/photos_flyingsky_overflow_button_size"
            android:scaleType="center"
            android:contentDescription="@null"
            app:srcCompat="@drawable/photos_quantum_gm_gs_share_vd_theme_18"/>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
