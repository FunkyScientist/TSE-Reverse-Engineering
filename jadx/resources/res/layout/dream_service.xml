<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.daydream.DreamViewFlipper xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dream_flipper"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/left"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/daydream_image_description"/>
    <ImageView
        android:id="@+id/center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/daydream_image_description"/>
    <include
        android:visibility="gone"
        layout="@layout/dream_empty"/>
</com.google.android.apps.photos.daydream.DreamViewFlipper>
