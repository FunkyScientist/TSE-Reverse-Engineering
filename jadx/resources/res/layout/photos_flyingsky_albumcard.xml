<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/photos_flyingsky_albumcard_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.apps.photos.memories.titlecard.MemoryTitleCardContainer
        android:id="@+id/photos_flyingsky_album_cover_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="24dp"
        android:paddingEnd="24dp"
        app:cardStyle="SQUARE">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_gravity="start"
            android:id="@+id/photos_flyingsky_albumcard_card"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/photos_flyingsky_albumcard_cover"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:scaleType="centerCrop"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/photos_flyingsky_albumcard_gradient"
                android:background="@drawable/photos_flyingsky_story_card_gradient"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <com.google.android.material.chip.Chip
                android:textAppearance="@style/TextAppearance.Photos.LabelMedium"
                android:textColor="@color/google_grey900"
                android:id="@+id/photos_flyingsky_albumcard_chip"
                android:clickable="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="16dp"
                android:layout_marginBottom="20dp"
                android:text="@string/photos_flyingsky_albumcard_chip_title"
                android:layout_marginHorizontal="16dp"
                android:layout_marginVertical="20dp"
                app:chipBackgroundColor="@color/google_white"
                app:chipCornerRadius="20dp"
                app:chipEndPadding="4dp"
                app:chipStartPadding="4dp"
                app:chipStrokeColor="@color/google_white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <FrameLayout
                android:id="@+id/overflow_button_tap_target"
                android:layout_width="@dimen/photos_flyingsky_miniminum_gar_dimension"
                android:layout_height="@dimen/photos_flyingsky_miniminum_gar_dimension"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                android:contentDescription="@string/photos_flyingsky_fragment_highlight_overflow_button_description"
                android:layout_marginEnd="16dp"
                android:layout_marginVertical="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:id="@+id/overflow_button"
                    android:background="@drawable/photos_flyingsky_transparent_overflow_button"
                    android:focusable="false"
                    android:clickable="false"
                    android:layout_width="@dimen/photos_flyingsky_overflow_button_size"
                    android:layout_height="@dimen/photos_flyingsky_overflow_button_size"
                    android:scaleType="center"
                    android:tint="@color/google_white"
                    android:contentDescription="@null"
                    android:backgroundTint="#80000000"
                    app:srcCompat="@drawable/photos_quantum_gm_ic_more_vert_vd_theme_18"/>
            </FrameLayout>
            <ImageView
                android:id="@+id/photos_flyingsky_albumcard_spotlight_image_scrim"
                android:background="@drawable/photos_flyingsky_spotlight_image_scrim"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHeight_percent="0.3"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:theme="@style/Widget.Photos.Overlay.Dark"
                android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
                android:textColor="?attr/colorSecondary"
                android:ellipsize="end"
                android:gravity="start"
                android:id="@+id/photos_albumcard_sharing_attributions_clickable"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="24dp"
                android:maxLines="2"
                android:lineSpacingExtra="1sp"
                android:textAlignment="viewStart"
                android:letterSpacing="0.01"
                android:paddingHorizontal="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
            <androidx.compose.p002ui.platform.ComposeView
                android:id="@+id/bottom_card_compose_module"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:paddingHorizontal="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.apps.photos.memories.titlecard.MemoryTitleCardContainer>
    <androidx.compose.p002ui.platform.ComposeView
        android:id="@+id/cover_images_row_compose_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"/>
    <ImageView
        android:id="@+id/bottom_wavy_divider"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="@dimen/photos_flyingsky_wavy_divider_height"
        android:layout_marginTop="20dp"
        android:contentDescription="@null"/>
</LinearLayout>
