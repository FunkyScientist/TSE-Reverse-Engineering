<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/udon_toolbar">
    <android.support.v7.widget.Toolbar
        android:layout_gravity="bottom"
        android:id="@+id/photos_photoeditor_udon_bottom_toolbar"
        android:background="@color/google_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/photos_photoeditor_udon_toolbar_height"
        android:contentInsetStart="0dp"
        android:contentInsetEnd="0dp"
        app:contentInsetEnd="0dp"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        style="?attr/actionBarStyle">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:filterTouchesWhenObscured="true">
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.google.android.material.button.MaterialButton
                    android:textColor="?attr/colorOnBackground"
                    android:id="@+id/photos_photoeditor_udon_cancel"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxWidth="150dp"
                    android:text="@string/cancel"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="16dp"
                    android:layout_alignParentStart="true"
                    android:backgroundTint="?attr/colorSecondaryContainer"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:rippleColor="?attr/colorPrimary"
                    style="@style/Widget.Photos.Button"/>
                <FrameLayout
                    android:id="@+id/photos_photoeditor_udon_toggle_preset_options_visibility_frame"
                    android:background="@drawable/photos_photoeditor_udon_button_background"
                    android:visibility="invisible"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <com.airbnb.lottie.LottieAnimationView
                        android:id="@+id/photos_photoeditor_udon_toggle_preset_options_menu_shimmer_bg"
                        android:background="@drawable/photos_photoeditor_udon_button_background"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_rawRes="@raw/photos_photoeditor_udon_button_background_animation"/>
                    <com.google.android.material.button.MaterialButton
                        android:layout_gravity="center"
                        android:id="@+id/photos_photoeditor_udon_toggle_preset_options_visibility"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:contentDescription="@string/photos_photoeditor_fragments_editor3_udon_expand_presets_desc"
                        android:backgroundTint="@android:color/transparent"
                        app:icon="@drawable/gs_edit_fix_auto_vd_theme_40"
                        app:iconGravity="textStart"
                        app:iconSize="24dp"
                        app:iconTint="?attr/colorOnSurface"
                        style="?attr/materialIconButtonFilledStyle"/>
                </FrameLayout>
                <FrameLayout
                    android:id="@+id/photos_photoeditor_udon_save_or_upsell_wrapper"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <Button
                        android:textColor="?attr/colorOnBackground"
                        android:id="@+id/photos_photoeditor_udon_g1_upsell_button"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxWidth="140dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="16dp"
                        style="@style/Widget.Material3.Button.TextButton"/>
                    <com.google.android.material.button.MaterialButton
                        android:enabled="false"
                        android:ellipsize="end"
                        android:id="@+id/photos_photoeditor_udon_save"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxWidth="150dp"
                        android:minWidth="48dp"
                        android:text="@string/photos_photoeditor_commonui_editor_action_save_copy"
                        android:maxLines="1"
                        android:paddingStart="24dp"
                        android:paddingEnd="24dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        style="@style/Widget.Photos.Button"/>
                </FrameLayout>
            </android.support.constraint.ConstraintLayout>
            <FrameLayout
                android:orientation="horizontal"
                android:id="@+id/photos_photoeditor_transform_controls"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <ImageView
                    android:layout_gravity="start|center_vertical"
                    android:id="@+id/photos_photoeditor_udon_transform_close_button"
                    android:background="@drawable/photos_photoeditor_udon_button_circle_background"
                    android:padding="8dp"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:contentDescription="@null"
                    android:layout_marginStart="16dp"
                    app:iconTint="@color/photos_photoeditor_commonui_icon_color"
                    app:srcCompat="@drawable/quantum_gm_ic_close_vd_theme_24"/>
                <FrameLayout
                    android:layout_gravity="end|center_vertical"
                    android:id="@+id/photos_photoeditor_udon_transform_effect_button_container"
                    android:background="@drawable/photos_photoeditor_udon_button_background"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginEnd="16dp">
                    <com.airbnb.lottie.LottieAnimationView
                        android:background="@drawable/photos_photoeditor_udon_button_background"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_rawRes="@raw/photos_photoeditor_udon_button_background_animation"/>
                    <ImageView
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@null"
                        app:srcCompat="@drawable/gs_arrow_forward_vd_theme_24"/>
                </FrameLayout>
            </FrameLayout>
            <ViewStub
                android:id="@+id/photos_photoeditor_udon_adjustment_actions_viewstub"
                android:layout="@layout/photos_photoeditor_udon_adjustment_actions"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <ViewStub
                android:id="@+id/photos_photoeditor_udon_dot_indicator_viewstub"
                android:layout="@layout/photos_photoeditor_udon_dot_indicator_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
    </android.support.v7.widget.Toolbar>
</merge>
