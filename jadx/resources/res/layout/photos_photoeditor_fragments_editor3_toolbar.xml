<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v7.widget.Toolbar
        android:layout_gravity="bottom"
        android:id="@+id/toolbar"
        android:background="@color/google_black"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:layout_margin="0dp"
        android:contentInsetStart="0dp"
        android:contentInsetEnd="0dp"
        app:contentInsetEnd="0dp"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        style="?attr/actionBarStyle">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:filterTouchesWhenObscured="true">
            <com.google.android.material.button.MaterialButton
                android:textColor="?attr/colorOnBackground"
                android:id="@+id/photos_photoeditor_fragments_editor3_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cancel"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                style="@style/Widget.Photos.Button.Borderless"/>
            <ViewStub
                android:id="@+id/photos_photoeditor_fragments_editor3_reset_viewstub"
                android:layout="@layout/photos_photoeditor_fragments_editor3_reset_button"
                android:inflatedId="@+id/photos_photoeditor_fragments_editor3_reset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ViewStub
                android:id="@+id/photos_photoeditor_fragments_editor3_done_viewstub"
                android:layout="@layout/photos_photoeditor_fragments_editor3_done_button"
                android:inflatedId="@+id/photos_photoeditor_fragments_editor3_done"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.google.android.material.button.MaterialButton
                android:id="@+id/photos_photoeditor_fragments_editor3_save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="48dp"
                android:text="@string/photos_photoeditor_commonui_editor_action_save"
                android:layout_centerVertical="true"
                android:paddingStart="24dp"
                android:paddingEnd="24dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:layout_alignParentEnd="true"
                style="@style/Widget.Photos.Button"/>
            <ViewStub
                android:id="@+id/photos_photoeditor_fragments_editor3_revert_viewstub"
                android:layout="@layout/photos_photoeditor_fragments_editor3_revert_button"
                android:inflatedId="@+id/photos_photoeditor_fragments_editor3_revert"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"/>
            <ViewStub
                android:id="@+id/photos_photoeditor_fragments_editor3_markup_actions_viewstub"
                android:layout="@layout/photos_photoeditor_fragments_editor3_markup_actions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
    </android.support.v7.widget.Toolbar>
</merge>
