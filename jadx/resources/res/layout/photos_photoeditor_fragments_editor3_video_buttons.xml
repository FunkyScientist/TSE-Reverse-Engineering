<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="16dp"
    android:paddingEnd="16dp">
    <com.google.android.material.button.MaterialButton
        android:gravity="start"
        android:layout_gravity="center_vertical"
        android:id="@+id/photos_photoeditor_fragments_editor3_mute"
        android:visibility="invisible"
        android:contentDescription="@string/photos_photoeditor_fragments_editor3_a11y_mute_audio"
        android:layout_marginStart="8dp"
        app:icon="@drawable/quantum_gm_ic_volume_up_vd_theme_24"
        style="@style/photos_photoeditor_fragments_editor3_video_image_button"/>
    <ViewStub
        android:gravity="start"
        android:layout_gravity="center_vertical"
        android:id="@+id/photos_photoeditor_chansey_view_stub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <com.google.android.material.button.MaterialButton
        android:gravity="start"
        android:layout_gravity="center_vertical"
        android:id="@+id/photos_photoeditor_fragments_editor3_stabilize"
        android:visibility="gone"
        android:contentDescription="@string/photos_photoeditor_commonui_a11y_stabilize_video"
        app:icon="@drawable/quantum_gm_ic_video_stable_vd_theme_24"
        style="@style/photos_photoeditor_fragments_editor3_video_image_button"/>
    <ViewStub
        android:layout_gravity="center_vertical"
        android:id="@+id/photos_photoeditor_fragments_editor3_slowpoke_entrypoint_icon_button_viewstub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <RelativeLayout
        android:gravity="end"
        android:layout_gravity="center_vertical"
        android:id="@+id/photos_photoeditor_fragments_editor3_video_hint_view_holder"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
