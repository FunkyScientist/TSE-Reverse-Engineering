<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="36dp"
    android:layout_height="36dp"
    android:layout_marginTop="@dimen/photos_partneraccount_onboarding_v2_face_item_top_spacing"
    android:layout_marginEnd="@dimen/photos_partneraccount_onboarding_v2_add_face_item_end_spacing">
    <com.google.android.material.button.MaterialButton
        android:textSize="12sp"
        android:textStyle="bold"
        android:textColor="?attr/colorOnSurface"
        android:id="@+id/add_face_button"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:maxLines="1"
        android:contentDescription="@null"
        android:textAlignment="center"
        android:elevation="2dp"
        android:backgroundTint="?attr/colorOnPrimary"
        style="@style/CircularButton"/>
</FrameLayout>
