<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/photos_assistant_cardui_photo_grid_padding">
    <com.google.android.apps.photos.assistant.cardui.CardPhotoView
        android:id="@+id/photo_0"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/photos_assistant_cardui_two_photos_min_height"
        android:layout_weight="1"
        android:contentDescription="@string/accessibility_photo_button"
        android:layout_marginEnd="@dimen/photos_assistant_cardui_photo_grid_padding"/>
    <com.google.android.apps.photos.assistant.cardui.CardPhotoView
        android:id="@+id/photo_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/photos_assistant_cardui_two_photos_min_height"
        android:layout_weight="1"
        android:contentDescription="@string/accessibility_photo_button"/>
</LinearLayout>
