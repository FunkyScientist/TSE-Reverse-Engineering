<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.album.enrichment.p004ui.EnrichmentAdornmentsLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center"
    android:id="@+id/enrichment_edit_mode_adornments"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:alpha="0">
    <RelativeLayout
        android:id="@+id/enrichment_dragged_overlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0">
        <View
            android:layout_gravity="start"
            android:id="@+id/enrichment_gradient_overlay_view"
            android:background="@drawable/enrichment_gradient"
            android:layout_width="140dp"
            android:layout_height="match_parent"/>
        <View
            android:layout_gravity="end"
            android:id="@+id/enrichment_flat_overlay_view"
            android:background="@drawable/enrichment_background"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toEndOf="@+id/enrichment_gradient_overlay_view"/>
    </RelativeLayout>
</com.google.android.apps.photos.album.enrichment.p004ui.EnrichmentAdornmentsLayout>
