<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/photos_photoeditor_udon_close_button"
        android:background="@drawable/photos_photoeditor_udon_button_circle_background"
        android:padding="8dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:contentDescription="@string/photos_photoeditor_fragments_editor3_cancel"
        android:layout_marginStart="16dp"
        android:layout_alignParentStart="true"
        app:iconTint="@color/photos_photoeditor_commonui_icon_color"
        app:srcCompat="@drawable/quantum_gm_ic_close_vd_theme_24"/>
    <ImageView
        android:id="@+id/photos_photoeditor_udon_done_button"
        android:background="@drawable/photos_photoeditor_udon_accept_circle_background"
        android:padding="8dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:contentDescription="@string/photos_strings_confirm_button"
        android:layout_marginEnd="16dp"
        android:layout_alignParentEnd="true"
        app:iconTint="@color/photos_photoeditor_commonui_icon_color"
        app:srcCompat="@drawable/quantum_gm_ic_check_vd_theme_24"/>
</merge>
