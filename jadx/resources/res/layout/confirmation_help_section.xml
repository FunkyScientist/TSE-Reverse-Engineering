<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textColor="?attr/photosOnSurfaceVariant"
        android:gravity="center"
        android:id="@+id/googler_feedback_text"
        android:paddingLeft="@dimen/photos_printingskus_photobook_confirmation_card_internal_padding"
        android:paddingRight="@dimen/photos_printingskus_photobook_confirmation_card_internal_padding"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:lineSpacingExtra="2dp"
        android:textDirection="locale"
        android:textAlignment="gravity"/>
    <TextView
        android:textColor="?attr/photosOnSurfaceVariant"
        android:gravity="center"
        android:id="@+id/help_text"
        android:paddingLeft="24dp"
        android:paddingRight="24dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="32dp"
        android:textDirection="locale"
        android:textAlignment="gravity"
        style="@style/quantum_text_menu_grey"/>
</LinearLayout>
