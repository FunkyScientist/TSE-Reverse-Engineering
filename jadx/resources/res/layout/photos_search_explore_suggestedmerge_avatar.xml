<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/photos_search_explore_suggestedmerge_avatar_size"
    android:layout_height="@dimen/photos_search_explore_suggestedmerge_avatar_size"
    android:layout_marginStart="@dimen/photos_search_explore_suggestedmerge_avatar_margin"
    android:layout_marginEnd="@dimen/photos_search_explore_suggestedmerge_avatar_margin">
    <ImageView
        android:id="@+id/avatar1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/photos_search_explore_suggestedmerge_avatar_title"/>
    <ImageView
        android:id="@+id/avatar2"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/photos_search_explore_suggestedmerge_avatar_title"/>
</FrameLayout>
