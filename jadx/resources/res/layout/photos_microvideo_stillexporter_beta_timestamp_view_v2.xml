<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:id="@+id/photos_microvideo_stillexporter_beta_timestamp_view"
    android:background="@drawable/photos_microvideo_stillexporter_beta_timestamp_bubble"
    android:layout_width="@dimen/photos_microvideo_stillexporter_beta_timestamp_bubble_width"
    android:layout_height="@dimen/photos_microvideo_stillexporter_beta_timestamp_bubble_height"
    android:elevation="3dp"
    android:clipToOutline="true">
    <TextView
        android:textAppearance="@style/photos_microvideo_stillexporter_beta_timestamp_text_style"
        android:gravity="top|center_horizontal"
        android:id="@+id/photos_microvideo_stillexporter_beta_timestamp_text_view_v2"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_centerVertical="true"
        android:lineSpacingExtra="2sp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"/>
</RelativeLayout>
