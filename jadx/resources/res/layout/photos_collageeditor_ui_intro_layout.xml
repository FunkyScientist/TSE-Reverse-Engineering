<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/photos_collageeditor_ui_overlay_color"
    android:paddingBottom="176dp"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:id="@+id/photos_collageeditor_ui_intro_tap"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/photos_collageeditor_ui_intro_page_layout"/>
    <include
        android:id="@+id/photos_collageeditor_ui_intro_pinch"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/photos_collageeditor_ui_intro_page_layout"/>
    <include
        android:id="@+id/photos_collageeditor_ui_intro_press"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@layout/photos_collageeditor_ui_intro_page_layout"/>
</FrameLayout>
