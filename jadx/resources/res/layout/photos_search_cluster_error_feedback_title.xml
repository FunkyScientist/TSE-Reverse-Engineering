<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="12dp"
    android:layout_marginBottom="20dp"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp">
    <TextView
        android:layout_gravity="start"
        android:id="@+id/photos_search_cluster_error_feedback_options_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        style="@style/Photos.Search.Cluster.Error.Feedback.View.Title"/>
    <TextView
        android:layout_gravity="start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/photos_search_removeresults_feedback_subtitle"
        style="@style/Photos.Search.Cluster.Error.Feedback.View.SubTitle"/>
</LinearLayout>
