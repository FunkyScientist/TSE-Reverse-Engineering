<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/photos_movies_v3_ui_title_card_text_fragment_root"
    android:background="@android:color/transparent"
    android:fitsSystemWindows="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.google.android.material.textfield.TextInputLayout
        android:layout_gravity="center"
        android:id="@+id/photos_movies_v3_ui_title_card_text_layout"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:boxBackgroundColor="@android:color/transparent"
        app:boxStrokeColor="@android:color/transparent"
        app:boxStrokeWidth="0dp"
        app:placeholderTextAppearance="@style/photos_movies_v3_ui_title_card_edit_text_style">
        <com.google.android.apps.photos.edittext.KeyboardDismissEditText
            android:id="@+id/photos_movies_v3_ui_title_card_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:imeOptions="flagNoExtractUi"
            style="@style/photos_movies_v3_ui_title_card_edit_text_style"/>
    </com.google.android.material.textfield.TextInputLayout>
</FrameLayout>
