<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:id="@+id/multi_person_overflow_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    android:minHeight="40dp"
    android:paddingStart="12dp"
    android:paddingEnd="8dp">
    <com.google.android.apps.photos.avatar.collage.CircularCollageView
        android:id="@+id/multi_person"
        android:layout_width="48dp"
        android:layout_height="48dp"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.Subhead2"
        android:textColor="?attr/photosOnSurfaceVariant"
        android:id="@+id/multi_person_subtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="8dp"
        android:paddingEnd="0dp"
        style="@style/photos_search_refinements_ui_overflow_unselected_icon_chip_materialThemeoverlay"/>
</LinearLayout>
