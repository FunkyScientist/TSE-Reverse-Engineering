<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="24dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="24dp">
    <TextView
        android:textAppearance="?attr/textAppearanceHeadlineSmall"
        android:textSize="24sp"
        android:textColor="?attr/colorOnSurface"
        android:gravity="center_vertical"
        android:id="@+id/photos_photoeditor_udon_error_dialog_title"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="2sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:textAppearance="?attr/textAppearanceBodyMedium"
        android:textSize="14sp"
        android:textColor="?attr/colorOnSurfaceVariant"
        android:textColorLink="?attr/colorOnSurfaceVariant"
        android:gravity="center_vertical"
        android:linksClickable="true"
        android:id="@+id/photos_photoeditor_udon_error_dialog_body_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:lineSpacingExtra="2sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_photoeditor_udon_error_dialog_title"/>
    <Button
        android:textColor="?attr/colorPrimary"
        android:id="@+id/photos_photoeditor_udon_error_dialog_ack_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/photos_photoeditor_udon_error_dialog_body_text"
        style="@style/Widget.Material3.Button.TextButton"/>
</android.support.constraint.ConstraintLayout>
