<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textAppearance="@style/quantum_text_headline_white"
        android:gravity="center"
        android:id="@+id/photos_cast_impl_media_message"
        android:background="@color/quantum_black_100"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <com.google.android.apps.photos.cast.impl.CastImageView
        android:id="@+id/photos_cast_impl_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitCenter"
        android:contentDescription="@string/photos_cast_impl_photo"/>
</FrameLayout>
