<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/photos_suggested_editor_coordinator"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/suggested_editor_preview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <ViewStub
        android:layout_gravity="bottom"
        android:id="@+id/photos_suggested_editor_document_adjustment_stub"
        android:layout="@layout/photos_suggestedactions_editor_document_adjustment"
        android:inflatedId="@+id/photos_suggested_editor_document_adjustment_inflated"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <android.widget.ProgressBar
        android:layout_gravity="center"
        android:id="@+id/suggestion_editor_spinner"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:alpha="0"
        style="@style/Widget.Photos.ProgressBar.IndeterminateWhite"/>
    <View
        android:id="@+id/photos_suggested_editor_navigation_bar_background"
        android:background="@color/google_black"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="0dp"/>
    <include layout="@layout/photos_suggestedactions_ui_suggested_editor_action_bar"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
