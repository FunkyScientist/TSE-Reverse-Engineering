<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/banner"
    android:background="?attr/photosPrimary"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.apps.photos.view.AccessibleClickSpanTextView
        android:textSize="14sp"
        android:textColor="@color/photos_daynight_white"
        android:gravity="center_vertical"
        android:id="@+id/banner_text"
        android:padding="16dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawablePadding="16dp"
        android:lineSpacingExtra="6dp"
        style="@style/photos_printingskus_photobook_theme_subhead2"/>
</FrameLayout>
