<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/all_photos_coordinator"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.app.FragmentContainerView
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <ViewStub
        android:id="@+id/all_photos_empty_state_layout_stub"
        android:layout="@layout/empty_all_photos_fragment_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <FrameLayout
        android:layout_gravity="end|bottom"
        android:id="@+id/hats_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/photos_allphotos_fragment_hats_margin"
        android:layout_marginTop="@dimen/photos_allphotos_fragment_hats_margin"
        android:layout_marginRight="@dimen/photos_allphotos_fragment_hats_margin"
        android:layout_marginBottom="@dimen/photos_allphotos_fragment_hats_margin"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
