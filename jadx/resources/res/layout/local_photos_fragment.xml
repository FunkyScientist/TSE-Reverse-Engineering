<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true">
    <android.support.v4.app.FragmentContainerView
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <include layout="@layout/photos_emptystate_container"/>
    <ViewStub
        android:id="@+id/local_folders_scrolling_toolbar_view_stub"
        android:layout="@layout/scrolling_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <include layout="@layout/scrim_view"/>
    <include layout="@layout/local_photos_error_bar_view"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
