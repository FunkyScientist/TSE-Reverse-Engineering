<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_gravity="end"
        android:id="@+id/photos_cloudstorage_ui_freetrial_impl_onboarding_close_button"
        android:background="?attr/selectableItemBackgroundBorderless"
        android:padding="12dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/photos_strings_dismiss_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/quantum_gm_ic_close_vd_theme_24"/>
    <include
        android:id="@+id/photos_cloudstorage_ui_freetrial_impl_onboarding_content"
        layout="@layout/photos_cloudstorage_ui_freetrial_impl_onboarding_content"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.Caption"
        android:textColor="@color/photos_daynight_grey800"
        android:gravity="center"
        android:layout_gravity="center"
        android:id="@+id/photos_cloudstorage_ui_freetrial_impl_onboarding_tos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:lineSpacingMultiplier="1.3"
        android:layout_marginHorizontal="16dp"
        android:layout_marginVertical="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
</LinearLayout>
