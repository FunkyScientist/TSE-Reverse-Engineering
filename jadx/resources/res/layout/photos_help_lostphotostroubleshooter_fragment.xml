<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/photos_help_lostphotostroubleshooter"
        android:paddingBottom="24dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="32dp"
            android:text="@string/photos_help_lost_photos_troubleshooter_heading"
            android:textAlignment="center"
            android:layout_marginHorizontal="32dp"
            style="@style/TextAppearance.Photos.HeadlineSmall"/>
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/photos_help_lostphotostroubleshooter_hero_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/photos_help_lostphotostroubleshooter_hero_image"
            android:contentDescription="@null"/>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_account"
            android:focusable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_account_avatar"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_account_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_account_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_account_subtitle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_account_avatar"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_account_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_account_avatar"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_account_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_device_folders"
            android:background="?android:attr/selectableItemBackground"
            android:focusable="true"
            android:clickable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_device_folders_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_folder_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_device_folders_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_device_folders_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_device_folders_subtitle"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_device_folders_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_device_folders_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_device_folders_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_device_folders_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_device_folders_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_device_folders_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_device_folders_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_device_folders_forward_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/quantum_gm_ic_arrow_forward_vd_theme_24"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_uncertain_dates"
            android:background="?android:attr/selectableItemBackground"
            android:focusable="true"
            android:visibility="gone"
            android:clickable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_calendar_today_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_uncertain_dates_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_subtitle"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_uncertain_dates_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_uncertain_dates_forward_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/quantum_gm_ic_arrow_forward_vd_theme_24"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_trash"
            android:background="?android:attr/selectableItemBackground"
            android:focusable="true"
            android:clickable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_trash_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_delete_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_trash_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_trash_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_trash_subtitle"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_trash_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_trash_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_trash_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_trash_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_trash_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_trash_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_trash_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_trash_forward_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/quantum_gm_ic_arrow_forward_vd_theme_24"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_backup"
            android:background="?android:attr/selectableItemBackground"
            android:focusable="true"
            android:clickable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_backup_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_cloud_off_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_backup_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_backup_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_backup_subtitle"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_backup_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_backup_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_backup_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_backup_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_backup_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_backup_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_backup_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_backup_forward_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/quantum_gm_ic_arrow_forward_vd_theme_24"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/photos_help_lostphotostroubleshooter_storage"
            android:background="?android:attr/selectableItemBackground"
            android:focusable="true"
            android:clickable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_storage_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_cloud_off_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_storage_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_storage_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_storage_subtitle"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_storage_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_storage_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_storage_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_storage_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/photos_help_lostphotostroubleshooter_storage_forward_arrow"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_storage_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_storage_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
            <ImageView
                android:id="@+id/photos_help_lostphotostroubleshooter_storage_forward_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@null"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/quantum_gm_ic_arrow_forward_vd_theme_24"/>
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:focusable="true"
            style="@style/LostPhotosTroubleShooterListItemContainer">
            <FrameLayout
                android:id="@+id/photos_help_lostphotostroubleshooter_other_devices_icon"
                android:background="@drawable/photos_help_lostphotostroubleshooter_icon_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:tint="@color/photos_daynight_blue600"
                    android:contentDescription="@null"
                    app:srcCompat="@drawable/quantum_gm_ic_sync_alt_vd_theme_24"/>
            </FrameLayout>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_other_devices_title"
                android:text="@string/photos_help_lost_photos_troubleshooter_other_devices_title"
                app:layout_constraintBottom_toTopOf="@+id/photos_help_lostphotostroubleshooter_other_devices_subtitle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_other_devices_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                style="@style/LostPhotosTroubleShooterTitle"/>
            <TextView
                android:id="@+id/photos_help_lostphotostroubleshooter_other_devices_subtitle"
                android:text="@string/photos_help_lost_photos_troubleshooter_other_devices_subtitle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/photos_help_lostphotostroubleshooter_other_devices_icon"
                app:layout_constraintTop_toBottomOf="@+id/photos_help_lostphotostroubleshooter_other_devices_title"
                style="@style/LostPhotosTroubleShooterSubtitle"/>
        </android.support.constraint.ConstraintLayout>
    </LinearLayout>
</ScrollView>
