<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/photos_edittext_edit_text_holder">
    <com.google.android.apps.photos.edittext.SelectableReEnableOnAttachTextView
        android:textAppearance="@style/TextAppearance.Photos.Body1"
        android:textColor="@color/photos_daynight_grey900"
        android:id="@+id/photos_edittext_edit_text_holder_static"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <EditText
        android:textAppearance="@style/TextAppearance.Photos.Body1"
        android:textColor="@color/photos_daynight_grey900"
        android:id="@+id/photos_edittext_edit_text_holder_editable"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:imeOptions="flagNoExtractUi|actionDone"/>
</merge>
