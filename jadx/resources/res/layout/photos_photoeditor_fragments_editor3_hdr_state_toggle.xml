<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:ellipsize="marquee"
        android:id="@+id/photos_photoeditor_fragments_editor3_hdr_state_toggle"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:visibility="gone"
        android:clickable="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxHeight="36dp"
        android:text="@string/photos_photoeditor_commonui_a11y_hdr_disabled"
        android:fontFamily="@font/google_sans_text"
        android:layout_marginEnd="16dp"
        android:paddingHorizontal="8dp"
        app:backgroundTint="@color/photos_photoeditor_fragments_editor3_hdr_state_toggle_background"
        app:collapsedSize="36dp"
        app:fabCustomSize="36dp"
        app:maxImageSize="20dp"
        app:shapeAppearance="?attr/shapeAppearanceCornerExtraLarge"/>
</FrameLayout>
