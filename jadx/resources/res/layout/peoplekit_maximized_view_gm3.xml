<?xml version="1.0" encoding="utf-8"?>
<com.google.android.libraries.social.peoplekit.common.analytics.PeopleKitControllerLoggingRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/peoplekit_maxview_whole_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/peoplekit_maxview_top_container"
            android:background="?attr/colorSurface"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/peoplekit_maxview_app_bar_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="@dimen/peoplekit_maxview_action_bar_height"
                android:paddingStart="0dp"
                android:paddingEnd="@dimen/peoplekit_maxview_action_bar_padding">
                <android.support.v7.widget.AppCompatImageView
                    android:id="@+id/peoplekit_maxview_close_button"
                    android:padding="@dimen/peoplekit_maxview_action_bar_padding"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="center"
                    android:tint="?attr/colorOnSurfaceVariant"
                    android:contentDescription="@string/peoplekit_maxview_close"
                    app:srcCompat="@drawable/quantum_ic_close_vd_theme_24"/>
                <com.google.android.material.button.MaterialButton
                    android:textAppearance="?attr/textAppearanceLabelLarge"
                    android:id="@+id/peoplekit_maxview_send_button_action_bar"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/peoplekit_maxview_send_button"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    style="@style/Widget.GoogleMaterial3.Button.TonalButton"/>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toStartOf="@+id/peoplekit_maxview_send_button_action_bar"
                    android:layout_toEndOf="@+id/peoplekit_maxview_close_button">
                    <TextView
                        android:textAppearance="?attr/textAppearanceTitleLarge"
                        android:ellipsize="end"
                        android:id="@+id/peoplekit_maxview_action_bar_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/peoplekit_maxview_select_people"
                        android:singleLine="true"/>
                    <TextView
                        android:textAppearance="?attr/textAppearanceBodyMedium"
                        android:ellipsize="end"
                        android:id="@+id/peoplekit_maxview_action_bar_subtitle"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:layout_below="@+id/peoplekit_maxview_action_bar_title"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/peoplekit_maxview_autocomplete"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/peoplekit_maxview_app_bar_layout"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/peoplekit_maxview_messagebar"
            android:background="?attr/colorSurface"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:translationY="@dimen/peoplekit_maxview_message_bar_translation"/>
        <RelativeLayout
            android:id="@+id/peoplekit_maxview_listview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/peoplekit_maxview_messagebar"
            android:layout_below="@+id/peoplekit_maxview_top_container"/>
    </RelativeLayout>
</com.google.android.libraries.social.peoplekit.common.analytics.PeopleKitControllerLoggingRelativeLayout>
