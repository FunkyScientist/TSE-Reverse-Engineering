<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:paddingTop="8dp"
    android:paddingBottom="8dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.Subtitle2"
        android:textColor="?attr/colorOnSurface"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/photos_search_guidedperson_same"/>
    <TextView
        android:textAppearance="@style/TextAppearance.Photos.Caption"
        android:textColor="?attr/photosOnSurfaceVariant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/photos_search_guidedperson_improve"/>
</LinearLayout>
