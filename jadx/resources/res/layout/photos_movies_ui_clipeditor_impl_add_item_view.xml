<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        style="@style/photos_movies_ui_clipeditor_impl_add_clip_divider"/>
    <TextView
        android:id="@+id/touch_handling_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/photos_movies_ui_clipeditor_impl_add_clips_item"
        style="@style/photos_movies_ui_clipeditor_impl_add_clip_button"/>
</LinearLayout>
