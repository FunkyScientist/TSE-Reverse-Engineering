<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.insetview.WindowInsetsView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/photos_partneraccount_onboarding_v2_send_invite"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            layout="@layout/photos_partneraccount_onboarding_v2_accordion_impl_accordion"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/bottomsheet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="@drawable/photos_partneraccount_onboarding_v2_send_bottom_button_divider"
            android:showDividers="end|middle|beginning"
            android:elevation="12dp">
            <Button
                android:id="@+id/select_partner_button"
                android:paddingLeft="24dp"
                android:paddingTop="10dp"
                android:paddingRight="24dp"
                android:paddingBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginRight="24dp"
                android:text="@string/photos_partneraccount_onboarding_v2_send_select_partner_button_text"
                android:layout_marginHorizontal="24dp"
                android:paddingHorizontal="24dp"
                android:paddingVertical="10dp"
                style="@style/Widget.Photos.Button"/>
            <Button
                android:id="@+id/confirm_invite_button"
                android:paddingLeft="24dp"
                android:paddingTop="10dp"
                android:paddingRight="24dp"
                android:paddingBottom="10dp"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginRight="24dp"
                android:layout_marginHorizontal="24dp"
                android:paddingHorizontal="24dp"
                android:paddingVertical="10dp"
                style="@style/Widget.Photos.Button"/>
        </LinearLayout>
    </LinearLayout>
</com.google.android.apps.photos.insetview.WindowInsetsView>
