<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linear_layout_root"
    android:background="?android:attr/colorBackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/upsell_fragment_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/management_v2_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:liftOnScroll="true">
            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:navigationContentDescription="@string/abc_action_bar_up_description"
                app:navigationIcon="@drawable/quantum_gm_ic_arrow_back_vd_theme_24"
                app:title="@string/subscriptions_storage"/>
        </com.google.android.material.appbar.AppBarLayout>
        <androidx.core.widget.NestedScrollView
            android:id="@+id/content"
            android:scrollbars="none"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:overScrollMode="never"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">
            <FrameLayout
                android:id="@+id/scroll_child"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ProgressBar
                    android:layout_gravity="center"
                    android:id="@+id/loading_circle"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    style="?android:attr/progressBarStyle"/>
                <TextView
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/data_error"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/subscriptions_data_failed_to_load"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    style="?attr/textAppearanceBody1"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/data_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_gravity="center_horizontal"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginTop="16dp"
                        android:scaleType="fitCenter"
                        android:contentDescription="@null"
                        app:srcCompat="@drawable/g1_google_one_logo"/>
                    <TextView
                        android:textColor="?attr/colorOnSurface"
                        android:gravity="center_horizontal"
                        android:id="@+id/header_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginEnd="32dp"
                        style="?attr/textAppearanceHeadline5"/>
                    <TextView
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:gravity="center_horizontal"
                        android:id="@+id/header_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginStart="32dp"
                        android:layout_marginEnd="32dp"
                        style="?attr/textAppearanceBody1"/>
                    <Button
                        android:layout_gravity="center_horizontal"
                        android:id="@+id/get_storage_button"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="24dp"
                        android:text="@string/subscriptions_get_storage"
                        android:layout_marginStart="32dp"
                        android:layout_marginEnd="32dp"/>
                    <com.google.android.libraries.subscriptions.management.p044v2.RecommendedPlanView
                        android:orientation="vertical"
                        android:id="@+id/recommended_plan_container"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="32dp"
                        android:layout_marginStart="24dp"
                        android:layout_marginEnd="24dp"/>
                    <FrameLayout
                        android:id="@+id/upsell_v2_fragment_container"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                    <View
                        android:id="@+id/main_section_divider"
                        android:background="?attr/colorOutline"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="24dp"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/account_avatar_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="24dp"
                        android:layout_marginStart="24dp"
                        android:layout_marginEnd="24dp">
                        <ImageView
                            android:id="@+id/account_avatar"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:scaleType="fitCenter"
                            android:importantForAccessibility="no"/>
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp">
                            <TextView
                                android:textColor="?attr/colorOnSurface"
                                android:id="@+id/account_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                style="?attr/textAppearanceBody1"/>
                            <TextView
                                android:textColor="?attr/colorOnSurfaceVariant"
                                android:id="@+id/account_email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                style="?attr/textAppearanceBody2"/>
                        </LinearLayout>
                    </LinearLayout>
                    <com.google.android.libraries.subscriptions.management.p044v2.StorageBreakdownView
                        android:id="@+id/storage_breakdown_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"/>
                    <com.google.android.libraries.subscriptions.management.p044v2.CollapsibleStorageBreakdownView
                        android:id="@+id/collapsible_storage_breakdown_view"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="30dp"/>
                    <Button
                        android:layout_gravity="center_horizontal"
                        android:id="@+id/clean_up_space"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="18dp"
                        android:layout_marginBottom="24dp"
                        android:text="@string/subscriptions_clean_up_space"
                        android:layout_marginStart="42dp"
                        android:layout_marginEnd="42dp"
                        style="?attr/materialButtonOutlinedStyle"/>
                    <Button
                        android:layout_gravity="center_horizontal"
                        android:id="@+id/clean_up_space_no_styling"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="18dp"
                        android:layout_marginBottom="24dp"
                        android:text="@string/subscriptions_clean_up_space"
                        android:layout_marginStart="42dp"
                        android:layout_marginEnd="42dp"/>
                    <com.google.android.libraries.subscriptions.management.p044v2.MiniStorageUpsellCardView
                        android:layout_gravity="bottom"
                        android:id="@+id/mini_storage_upsell_card"
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="24dp"
                        android:layout_marginStart="24dp"
                        android:layout_marginEnd="24dp"/>
                </LinearLayout>
            </FrameLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</FrameLayout>
