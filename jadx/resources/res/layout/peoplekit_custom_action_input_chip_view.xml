<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.chip.Chip xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:textSize="14sp"
    android:textColor="?attr/colorOnPrimary"
    android:ellipsize="end"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:checkable="false"
    android:lineSpacingExtra="2sp"
    android:layout_marginEnd="8dp"
    app:checkedIconVisible="false"
    app:chipCornerRadius="100dp"
    app:chipEndPadding="24dp"
    app:chipIconSize="@dimen/peoplekit_custom_action_chip_icon_size"
    app:chipMinHeight="36dp"
    app:chipStartPadding="0dp"
    app:chipStrokeColor="@android:color/transparent"
    app:closeIconVisible="false"
    app:ensureMinTouchTargetSize="false"
    app:iconStartPadding="16dp"
    style="@style/Widget.GoogleMaterial.Chip.Input"/>
