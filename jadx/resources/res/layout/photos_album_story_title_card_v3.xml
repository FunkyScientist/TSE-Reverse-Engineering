<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.memories.titlecard.MemoryTitleCardContainer xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/story_card_container"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="16dp"
    android:paddingEnd="16dp"
    app:cardStyle="THREE_BY_TWO">
    <com.google.android.apps.photos.album.titlecard.AlbumStoryTitleCard
        android:layout_gravity="start"
        android:id="@+id/photos_story_card"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/photos_story_card_cover"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleType="centerCrop"
            android:contentDescription="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.compose.p002ui.platform.ComposeView
            android:id="@+id/highlight_preview_container"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:contentDescription="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <include
            android:id="@+id/photos_story_card_play_icon"
            android:visibility="gone"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_margin="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/photos_album_story_title_card_play_icon"/>
        <ImageView
            android:background="@drawable/photos_album_story_title_card_gradient"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:contentDescription="@null"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/photos_album_story_title_card_top_scrim"
            android:background="@drawable/photos_album_story_title_card_top_gradient"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:contentDescription="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Button
            android:id="@+id/photos_album_story_title_card_remove_highlight"
            android:visibility="gone"
            android:layout_marginTop="8dp"
            android:text="@string/photos_album_story_titlecard_remove_highlight"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/Photos.AlbumStoryTitleCard.EditButton"/>
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.Button"
            android:textSize="11sp"
            android:textColor="@color/google_white"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/photos_story_card_highlight_info"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:maxLines="3"
            android:paddingHorizontal="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/photos_album_story_title_card_remove_highlight"
            app:layout_constraintEnd_toStartOf="@+id/photos_album_story_title_card_edit_highlight"
            app:layout_constraintStart_toEndOf="@+id/photos_album_story_title_card_remove_highlight"
            app:layout_constraintTop_toTopOf="@+id/photos_album_story_title_card_remove_highlight"/>
        <Button
            android:id="@+id/photos_album_story_title_card_edit_highlight"
            android:visibility="gone"
            android:layout_marginTop="8dp"
            android:text="@string/photos_album_story_titlecard_edit_highlight"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/Photos.AlbumStoryTitleCard.EditButton"/>
        <EditText
            android:textColor="@color/google_white"
            android:textColorHint="@color/photos_album_titlecard_edit_title_hint"
            android:ellipsize="end"
            android:gravity="start"
            android:id="@+id/photos_story_card_title_editing"
            android:background="@drawable/photos_album_story_edit_title_border"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="24dp"
            android:hint="@string/photos_strings_untitled_album_hint_text"
            android:maxLength="150"
            android:inputType="textMultiLine"
            android:imeOptions="actionDone"
            android:textAlignment="viewStart"
            android:layout_marginHorizontal="16dp"
            android:autofillHints=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            style="@style/Photos.Album.StoryTitleCard.CollectionTitleV2"/>
        <TextView
            android:textColor="@color/google_white"
            android:ellipsize="end"
            android:gravity="start"
            android:id="@+id/photos_story_card_title"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="24dp"
            android:maxLength="150"
            android:textAlignment="viewStart"
            android:paddingHorizontal="16dp"
            app:layout_constraintBottom_toTopOf="@+id/photos_story_card_title_editing"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_goneMarginBottom="24dp"
            style="@style/Photos.Album.StoryTitleCard.CollectionTitleV2"/>
        <TextView
            android:textColor="@color/google_white"
            android:ellipsize="end"
            android:gravity="start"
            android:id="@+id/photos_story_card_subtitle"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="4dp"
            android:maxLines="2"
            android:textAlignment="viewStart"
            android:paddingHorizontal="16dp"
            app:layout_constraintBottom_toTopOf="@+id/photos_story_card_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            style="@style/Photos.Album.Titlecard.CollectionSubtitleV2"/>
    </com.google.android.apps.photos.album.titlecard.AlbumStoryTitleCard>
</com.google.android.apps.photos.memories.titlecard.MemoryTitleCardContainer>
