<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="0">
    <TableRow
        android:paddingLeft="16dp"
        android:paddingTop="9dp"
        android:paddingRight="16dp"
        android:paddingBottom="9dp"
        android:paddingHorizontal="16dp"
        android:paddingVertical="9dp">
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:layout_width="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_feature_label"
            android:layout_weight="1"
            android:hyphenationFrequency="fullFast"
            android:layout_marginHorizontal="9dp"/>
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:id="@+id/not_backed_up_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_not_backed_up"
            android:maxEms="6"
            android:hyphenationFrequency="fullFast"
            android:layout_marginHorizontal="9dp"/>
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:id="@+id/backed_up_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_backed_up"
            android:maxEms="6"
            android:hyphenationFrequency="fullFast"
            android:layout_marginHorizontal="9dp"/>
    </TableRow>
    <TableRow
        android:background="?attr/photosUncheckedBackground"
        android:paddingLeft="16dp"
        android:paddingTop="9dp"
        android:paddingRight="16dp"
        android:paddingBottom="9dp"
        android:paddingHorizontal="16dp"
        android:paddingVertical="9dp">
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:layout_width="0dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_safety"
            android:layout_weight="1"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/colorOutline"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_no_icon_description"
            app:srcCompat="@drawable/gs_close_vd_24"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/colorPrimary"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_yes_icon_description"
            app:srcCompat="@drawable/gs_check_vd_theme_24"/>
    </TableRow>
    <TableRow
        android:paddingLeft="16dp"
        android:paddingTop="9dp"
        android:paddingRight="16dp"
        android:paddingBottom="9dp"
        android:paddingHorizontal="16dp"
        android:paddingVertical="9dp">
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:layout_width="0dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_memories"
            android:layout_weight="1"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/colorOutline"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_no_icon_description"
            app:srcCompat="@drawable/gs_close_vd_24"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/photosPrimary"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_yes_icon_description"
            app:srcCompat="@drawable/gs_check_vd_theme_24"/>
    </TableRow>
    <TableRow
        android:background="?attr/photosUncheckedBackground"
        android:paddingLeft="16dp"
        android:paddingTop="9dp"
        android:paddingRight="16dp"
        android:paddingBottom="9dp"
        android:paddingHorizontal="16dp"
        android:paddingVertical="9dp">
        <TextView
            android:textAppearance="@style/TextAppearance.Photos.LabelSmall"
            android:layout_width="0dp"
            android:text="@string/photos_autobackuppromos_bbd_compare_search"
            android:layout_weight="1"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/colorOutline"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_no_icon_description"
            app:srcCompat="@drawable/gs_close_vd_24"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:tint="?attr/photosPrimary"
            android:layout_weight="0"
            android:contentDescription="@string/photos_autobackuppromos_bbd_compare_yes_icon_description"
            app:srcCompat="@drawable/gs_check_vd_theme_24"/>
    </TableRow>
</TableLayout>
