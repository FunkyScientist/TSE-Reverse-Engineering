<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.photos.touchcapture.TouchCaptureView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/touch_capture_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/album_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <include layout="@layout/empty_view_layout"/>
    <include layout="@layout/shared_collection_offline_view"/>
    <include layout="@layout/shared_collection_not_found_layout"/>
    <FrameLayout
        android:id="@+id/photo_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <FrameLayout
        android:id="@+id/envelope_settings_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</com.google.android.apps.photos.touchcapture.TouchCaptureView>
