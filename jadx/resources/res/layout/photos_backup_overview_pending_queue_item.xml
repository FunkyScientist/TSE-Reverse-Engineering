<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/photos_backup_overview_pending_queue_item_layout"
    android:layout_width="0dp"
    android:layout_height="0dp">
    <com.google.android.apps.photos.view.RoundedCornerImageView
        android:id="@+id/photos_backup_overview_pending_queue_item_thumbnail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/photos_carousel_common_view_all_button"/>
    <ImageView
        android:gravity="center"
        android:id="@+id/photos_backup_overview_pending_queue_item_arrow_icon"
        android:visibility="gone"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:tint="@color/google_white"
        android:layout_centerInParent="true"
        android:selectable="false"
        android:contentDescription="@null"
        app:srcCompat="@drawable/photos_backup_overview_view_all_arrow"/>
</RelativeLayout>
